a=list(map(int,input().split()))
b=list(map(int,input().split()))
c=d=0
for i in range(len(a)):
    if a[i]>b[i]:
        c=c+1
    elif a[i]<b[i]:
        d=d+1
print(c,d)
    

  or
a=list(map(int,input().split()))
b=list(map(int,input().split()))
c=[0,0]
for i in range(len(a)):
if a[i]>b[i]:
        c[0]=c[0]+1
    elif a[i]<b[i]:
        d[1]=d[1]+1
print(*c)
